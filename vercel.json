{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "SERPAPI_KEY": "@serpapi_key"
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}